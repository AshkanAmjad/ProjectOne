@model IQueryable<ViewModels.Models.Comment.CommentViewModel>

@{ if (Model.Count() != 0)
    {
    <ul>
        @foreach (var item in Model)
        {
            <li>
                @item.UserId :
            </li>
            <li>
                @item.Content
            </li>
            <li>
                @item.CreateDate
            </li>
        }
    </ul>
    }
}

